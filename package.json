{
  "name": "esp-monorepo-tests",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/Espressive/esp-monorepo-tests.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "app1": "yarn --cwd apps/cra-app1 start",
    "app2": "yarn --cwd apps/cra-app2 start",
    "app3": "yarn --cwd apps/cra-app3 start",
    "tsapp": "yarn --cwd apps/tsapp start",
    "clean:modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:files": "rm -rf .pnp* yarn.lock package-lock.json",
    "reset": "yarn run clean:modules && yarn run clean:files"
  },
  "dependencies": {},
  "devDependencies": {},
  "workspaces": [
    "apps/*",
    "comps/*",
    "cra-comps/*"
  ]
}
